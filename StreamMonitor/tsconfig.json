{
  "compilerOptions": {
    "target": "esnext",
"lib": ["esnext"]
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "server",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
"types": ["node"], // include node types
  },
  "include": ["server/**/*.ts"]
}
